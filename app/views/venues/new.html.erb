<h1>New venue</h1>

<% form_for(@venue) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name, 'Name' %><br />
    <%= f.text_field :name , :size => 40%>
  </p>
  <p>
    <%= f.label :address, 'Address' %><br />
    <%= f.text_field :address , :size => 40%>
  </p>
  <p>
    <%= f.label :phone, 'Phone' %><br />
    <%= f.text_field :phone, :size => 40 %>
  </p>
  <p>
    <%= f.label :website, 'Website' %><br />
    <%= f.text_field :website, :size => 40 %>
  </p>

<p>
    <%= f.label :location_id, 'Location ID' %><br />
    <%= f.text_field :location_id, :size => 40 %>
  </p>


  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', venues_path %>
